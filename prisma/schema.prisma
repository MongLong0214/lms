datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Course {
  id String @id @default(uuid())
  useId String
  title String @db.Text
  description String? @db.Text
  imgeUrl String? @db.Text
}